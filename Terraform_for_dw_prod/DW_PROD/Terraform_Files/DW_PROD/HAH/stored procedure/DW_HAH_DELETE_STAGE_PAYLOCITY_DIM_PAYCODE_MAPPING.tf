resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_PAYLOCITY_DIM_PAYCODE_MAPPING" {
	name ="DELETE_STAGE_PAYLOCITY_DIM_PAYCODE_MAPPING"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

	BEGIN

	DELETE FROM HAH.DIM_PAYCODE_MAPPING WHERE SOURCE_SYSTEM_ID = 10 AND PAYCODE_MAPPING_KEY
    NOT IN (SELECT PAYCODE_MAPPING_KEY FROM STAGE.PAYLOCITY_DIM_PAYCODE_MAPPING);

	RETURN ''Message : Done'';
	END;

 EOT
}

