resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_PAYOR_CONTRACT_UI_DIM_PARENT_PARTNER" {
	name ="DELETE_STAGE_PAYOR_CONTRACT_UI_DIM_PARENT_PARTNER"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

DECLARE
    RETURN_RESULT VARCHAR;
BEGIN
DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.DIM_PARENT_PARTNER WHERE SOURCE_SYSTEM_ID = 28 AND SYSTEM_CODE =''Payor_Contract UI''
AND PARENT_PARTNER_KEY NOT IN (SELECT PARENT_PARTNER_KEY FROM DW_${var.SF_ENVIRONMENT}."STAGE".PAYOR_CONTRACT_UI_DIM_PARENT_PARTNER);
SELECT CONCAT(''Message : '',"number of rows deleted", '' Rows Deleted.'') into :return_result FROM TABLE(RESULT_SCAN(LAST_QUERY_ID()));
return return_result;
END;

 EOT
}

