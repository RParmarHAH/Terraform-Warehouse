resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_VIVENTIUM_FACT_PAYROLL_DETAILS" {
	name ="DELETE_STAGE_VIVENTIUM_FACT_PAYROLL_DETAILS"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN
 DELETE FROM HAH.FACT_PAYROLL_DETAILS WHERE SOURCE_SYSTEM_ID = 34 AND PAYROLL_DETAIL_KEY
  NOT IN (SELECT PAYROLL_DETAIL_KEY FROM STAGE.VIVENTIUM_FACT_PAYROLL_DETAILS);
return ''Done'';
END;
  
  

 EOT
