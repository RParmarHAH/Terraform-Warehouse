resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_OSHAH_FACT_VISIT" {
	name ="DELETE_STAGE_OSHAH_FACT_VISIT"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN
DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.FACT_VISIT WHERE SOURCE_SYSTEM_ID =17 AND SYSTEM_CODE ILIKE ''%OSHAH%'' AND VISIT_KEY
  NOT IN (SELECT VISIT_KEY FROM DW_${var.SF_ENVIRONMENT}.STAGE.OSHAH_FACT_VISIT);
RETURN ''SUCCESS'';
END;

 EOT
}

