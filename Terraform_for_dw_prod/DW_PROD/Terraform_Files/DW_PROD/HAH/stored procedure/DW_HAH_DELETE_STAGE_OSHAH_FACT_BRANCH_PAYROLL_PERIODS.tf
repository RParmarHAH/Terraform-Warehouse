resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_OSHAH_FACT_BRANCH_PAYROLL_PERIODS" {
	name ="DELETE_STAGE_OSHAH_FACT_BRANCH_PAYROLL_PERIODS"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN 
DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.FACT_BRANCH_PAYROLL_PERIODS 
	WHERE BRANCH_KEY IN (SELECT BRANCH_KEY FROM DW_${var.SF_ENVIRONMENT}."STAGE".OSHAH_DIM_BRANCH)
	AND BRANCH_PAYROLL_PERIODS_KEY NOT IN (select BRANCH_PAYROLL_PERIODS_KEY from DW_${var.SF_ENVIRONMENT}."STAGE".OSHAH_FACT_BRANCH_PAYROLL_PERIODS);
RETURN ''SUCCESS'';
END;

 EOT
}

