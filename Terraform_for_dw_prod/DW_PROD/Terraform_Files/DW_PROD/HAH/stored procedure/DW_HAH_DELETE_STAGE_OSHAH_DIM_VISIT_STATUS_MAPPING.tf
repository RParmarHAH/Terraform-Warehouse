resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_OSHAH_DIM_VISIT_STATUS_MAPPING" {
	name ="DELETE_STAGE_OSHAH_DIM_VISIT_STATUS_MAPPING"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN
DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.DIM_VISIT_STATUS_MAPPING WHERE SOURCE_SYSTEM_ID =17 AND SYSTEM_CODE ILIKE ''%OSHAH%'' AND VISIT_STATUS_KEY
  NOT IN (SELECT VISIT_STATUS_KEY FROM DW_${var.SF_ENVIRONMENT}.STAGE.OSHAH_DIM_VISIT_STATUS_MAPPING);
RETURN ''SUCCESS'';
END;

 EOT
}

