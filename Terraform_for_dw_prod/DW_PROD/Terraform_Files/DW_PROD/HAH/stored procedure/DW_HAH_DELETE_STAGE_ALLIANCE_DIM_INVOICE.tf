resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_ALLIANCE_DIM_INVOICE" {
	name ="DELETE_STAGE_ALLIANCE_DIM_INVOICE"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(1677216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN
DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.DIM_INVOICE WHERE SOURCE_SYSTEM_ID=19
	AND SYSTEM_CODE=''ALLIANCE'' AND INVOICE_KEY
	NOT IN (SELECT INVOICE_KEY FROM DW_${var.SF_ENVIRONMENT}.STAGE.ALLIANCE_DIM_INVOICE);
RETURN ''SUCCESS'';
END;

 EOT
}

