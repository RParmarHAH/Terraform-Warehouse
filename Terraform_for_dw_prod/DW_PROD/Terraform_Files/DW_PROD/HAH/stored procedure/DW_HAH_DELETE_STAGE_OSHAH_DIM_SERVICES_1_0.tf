resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_OSHAH_DIM_SERVICES_1_0" {
	name ="DELETE_STAGE_OSHAH_DIM_SERVICES_1_0"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

BEGIN
  DELETE FROM DW_${var.SF_ENVIRONMENT}.HAH.DIM_SERVICES_1_0 WHERE SOURCE_SYSTEM_ID =17 AND SYSTEM_CODE LIKE ''%OSHAH%'' AND SERVICE_KEY
  NOT IN (SELECT SERVICE_KEY FROM DW_${var.SF_ENVIRONMENT}.STAGE.OSHAH_DIM_SERVICES_1_0);
  
RETURN ''SUCCESS'';
END;

 EOT
}

