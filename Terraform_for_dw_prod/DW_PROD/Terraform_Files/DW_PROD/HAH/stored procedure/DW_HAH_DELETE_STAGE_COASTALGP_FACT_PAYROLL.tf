resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_COASTALGP_FACT_PAYROLL" {
	name ="DELETE_STAGE_COASTALGP_FACT_PAYROLL"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

DECLARE
  RETURN_RESULT VARCHAR;
BEGIN
	DELETE FROM HAH.FACT_PAYROLL WHERE SOURCE_SYSTEM_ID = 11 AND PAYROLL_KEY
    NOT IN (SELECT PAYROLL_KEY FROM STAGE.COASTALGP_FACT_PAYROLL);
   
	SELECT CONCAT(''Message : '',"number of rows deleted", '' Rows Deleted.'') into :return_result FROM TABLE(RESULT_SCAN(LAST_QUERY_ID()));
	RETURN RETURN_RESULT;
END;

 EOT
}

