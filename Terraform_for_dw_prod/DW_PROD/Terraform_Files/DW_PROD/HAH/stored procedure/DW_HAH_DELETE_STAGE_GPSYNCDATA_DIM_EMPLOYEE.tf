resource "snowflake_procedure" "DW_HAH_DELETE_STAGE_GPSYNCDATA_DIM_EMPLOYEE" {
	name ="DELETE_STAGE_GPSYNCDATA_DIM_EMPLOYEE"
	database = "DW_${var.SF_ENVIRONMENT}"
	schema = "HAH"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT


	DECLARE 
		RES RESULTSET;
	BEGIN
		DELETE FROM HAH.DIM_EMPLOYEE WHERE SOURCE_SYSTEM_ID = 5 AND EMPLOYEE_KEY
    NOT IN (SELECT EMPLOYEE_KEY FROM STAGE.GPSYNCDATA_DIM_EMPLOYEE);
  
RETURN ''DONE'';
END;

 EOT
}

