resource "snowflake_procedure" "DISC_ALAYACARE_GET_EMPLOYEE_EXPERIENCED" {
	name ="GET_EMPLOYEE_EXPERIENCED"
	database = "DISC_${var.SF_ENVIRONMENT}"
	schema = "ALAYACARE"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

DECLARE
--*****************************************************************************************************************************
-- NAME:  GET_EMPLOYEE_EXPERIENCED 
--
-- PURPOSE: USING THIS SP FOR DISCOVERY TABLE LOAD
--
-- DEVELOPMENT LOG:
-- DATE        		AUTHOR                	NOTES:
-- ----------  		-------------------   	-----------------------------------------------------------------------------------------------
-- 2021-11-11		UNKNOWN					INITIAL DEVELOPMENT
-- 2023-09-05  		RAVI SUTHAR            	MIGRATED JavaScript to SQL
--*****************************************************************************************************************************
    return_result varchar(1000);
BEGIN
	
    INSERT OVERWRITE INTO ALAYACARE.EMPLOYEE_EXPERIENCED (
        ID, CLIENT_ID, EMPLOYEE_ID, VISITS_TOTAL, VISITS_WITH_CONTINUITY, CONTINUITY, PROPERTIES_DATE_COMPUTED, PROPERTIES_ID_AUTO, PROPERTIES_VISITS_TOTAL, PROPERTIES_VISITS_WITH_CONTINUITY, PROPERTIES_OP, PROPERTIES_EMPLOYEE_ID, PROPERTIES_RANK, PROPERTIES_CLIENT_ID)
        SELECT ID AS ID, CLIENT_ID AS CLIENT_ID, EMPLOYEE_ID AS EMPLOYEE_ID, VISITS_TOTAL AS VISITS_TOTAL, VISITS_WITH_CONTINUITY AS VISITS_WITH_CONTINUITY, CONTINUITY AS CONTINUITY, PROPERTIES:date_computed::STRING AS PROPERTIES_DATE_COMPUTED, PROPERTIES:id_auto::STRING AS PROPERTIES_ID_AUTO, PROPERTIES:visits_total::STRING AS PROPERTIES_VISITS_TOTAL, PROPERTIES:visits_with_continuity::STRING AS PROPERTIES_VISITS_WITH_CONTINUITY, PROPERTIES:Op::STRING AS PROPERTIES_OP, PROPERTIES:employee_id::STRING AS PROPERTIES_EMPLOYEE_ID, PROPERTIES:rank::STRING AS PROPERTIES_RANK, PROPERTIES:client_id::STRING AS PROPERTIES_CLIENT_ID 
        FROM ALAYACARE_${var.SF_ENVIRONMENT}UCTION."5C3EE5FE-45A5-4DA7-A35B-BD7CDAFE3548".SHARED_EMPLOYEE_EXPERIENCED;

	SELECT CONCAT(''Message : '',"number of rows inserted",'' Rows Inserted.'') into :return_result FROM TABLE(RESULT_SCAN(LAST_QUERY_ID()));
	return return_result;
END;

 EOT
}

