resource "snowflake_procedure" "DISC_ALAYACARE_GET_USER_RATES" {
	name ="GET_USER_RATES"
	database = "DISC_${var.SF_ENVIRONMENT}"
	schema = "ALAYACARE"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

DECLARE
--*****************************************************************************************************************************
-- NAME:  GET_USER_RATES 
--
-- PURPOSE: USING THIS SP FOR DISCOVERY TABLE LOAD
--
-- DEVELOPMENT LOG:
-- DATE        		AUTHOR                	NOTES:
-- ----------  		-------------------   	-----------------------------------------------------------------------------------------------
-- 2021-11-11		UNKNOWN					INITIAL DEVELOPMENT
-- 2023-09-05  		RAVI SUTHAR            	MIGRATED JavaScript to SQL
--*****************************************************************************************************************************
    return_result varchar(1000);
BEGIN
	
    INSERT OVERWRITE INTO ALAYACARE.USER_RATES (
        ID, EMPLOYEE_ID, CUSTOM_RATE, PAY_CODE_RATE, RATE_START_DATE, PAY_CODE, RATE_TYPE, PROPERTIES_USER_RATES_ID, PROPERTIES_USER_RATES_PAYROLL_ITEM_ID, PROPERTIES_USER_RATES_RATE, PROPERTIES_USER_RATES_OP, PROPERTIES_USER_RATES_START_DATE, PROPERTIES_USER_RATES_EMPLOYEE_ID, PROPERTIES_USER_RATES_OVERRIDE_TYPE)
        SELECT ID AS ID, EMPLOYEE_ID AS EMPLOYEE_ID, CUSTOM_RATE AS CUSTOM_RATE, PAY_CODE_RATE AS PAY_CODE_RATE, RATE_START_DATE AS RATE_START_DATE, PAY_CODE AS PAY_CODE, RATE_TYPE AS RATE_TYPE, PROPERTIES_USER_RATES:id::STRING AS PROPERTIES_USER_RATES_ID, PROPERTIES_USER_RATES:payroll_item_id::STRING AS PROPERTIES_USER_RATES_PAYROLL_ITEM_ID, PROPERTIES_USER_RATES:rate::STRING AS PROPERTIES_USER_RATES_RATE, PROPERTIES_USER_RATES:Op::STRING AS PROPERTIES_USER_RATES_OP, PROPERTIES_USER_RATES:start_date::STRING AS PROPERTIES_USER_RATES_START_DATE, PROPERTIES_USER_RATES:employee_id::STRING AS PROPERTIES_USER_RATES_EMPLOYEE_ID, PROPERTIES_USER_RATES:override_type::STRING AS PROPERTIES_USER_RATES_OVERRIDE_TYPE 
        FROM ALAYACARE_${var.SF_ENVIRONMENT}UCTION."5C3EE5FE-45A5-4DA7-A35B-BD7CDAFE3548".SHARED_USER_RATES;

	SELECT CONCAT(''Message : '',"number of rows inserted",'' Rows Inserted.'') into :return_result FROM TABLE(RESULT_SCAN(LAST_QUERY_ID()));
	return return_result;
END;

 EOT
}

