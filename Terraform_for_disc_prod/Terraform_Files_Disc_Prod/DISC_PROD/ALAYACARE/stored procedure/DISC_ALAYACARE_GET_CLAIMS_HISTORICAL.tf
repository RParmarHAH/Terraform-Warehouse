	statement = <<-EOT

DECLARE
--*****************************************************************************************************************************
-- NAME:  GET_CLAIMS_HISTORICAL 
--
-- PURPOSE: USING THIS SP FOR DISCOVERY TABLE LOAD
--
-- DEVELOPMENT LOG:
-- DATE        		AUTHOR                	NOTES:
-- ----------  		-------------------   	-----------------------------------------------------------------------------------------------
-- 2021-11-11		UNKNOWN					INITIAL DEVELOPMENT
-- 2023-09-05  		RAVI SUTHAR            	MIGRATED JavaScript to SQL
--*****************************************************************************************************************************
    return_result varchar(1000);
BEGIN
	
    INSERT OVERWRITE INTO ALAYACARE.CLAIMS_HISTORICAL (
        CLAIM_ID, STATUS, REVISION_BY, CLIENT_ID, FUNDER_ID, BILLCODE_ID, SERVICED_AT_DATE, ADJUDICATION_NUMBER, ISSUES, UPDATE_FLOW, FREQUENCY, REVENUE_CODE, REVENUE_CODE_DESCRIPTION, PATIENT_DISCHARGE_STATUS, CLAIM_PRIORITY_TYPE, DELAY_REASON_CODE, PROCEDURE_CODE, PROCEDURE_MODIFIER, QUANTITY, TOTAL_BILLED, TOTAL_PAID, TOTAL_ADJUSTED, BALANCE, REVISION_FROM)
