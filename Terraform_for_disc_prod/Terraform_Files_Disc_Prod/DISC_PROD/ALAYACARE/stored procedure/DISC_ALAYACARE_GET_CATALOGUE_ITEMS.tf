resource "snowflake_procedure" "DISC_ALAYACARE_GET_CATALOGUE_ITEMS" {
	name ="GET_CATALOGUE_ITEMS"
	database = "DISC_${var.SF_ENVIRONMENT}"
	schema = "ALAYACARE"
	language  = "SQL"
	return_type = "VARCHAR(16777216)"
	execute_as = "OWNER"
	statement = <<-EOT

DECLARE
--*****************************************************************************************************************************
-- NAME:  GET_CATALOGUE_ITEMS 
--
-- PURPOSE: USING THIS SP FOR DISCOVERY TABLE LOAD
--
-- DEVELOPMENT LOG:
-- DATE        		AUTHOR                	NOTES:
-- ----------  		-------------------   	-----------------------------------------------------------------------------------------------
-- 2021-11-11		UNKNOWN					INITIAL DEVELOPMENT
-- 2023-09-05  		RAVI SUTHAR            	MIGRATED JavaScript to SQL
--*****************************************************************************************************************************
    return_result varchar(1000);
BEGIN
	
    INSERT OVERWRITE INTO ALAYACARE.CATALOGUE_ITEMS (
        CATALOGUEITEM_ID, CATALOGUEITEM_CODE, CATALOGUEITEM_STATUS, CATALOGUEITEM_DESCRIPTION, CATALOGUEITEM_TYPE_CODE, CATALOGUEITEM_TYPE_DESCRIPTION, CATALOGUEITEM_UNIT, CATALOGUEITEM_CATEGORY, CATALOGUEITEM_PRICE_PER_ITEM, CATALOGUEITEM_BILLED_PER_ITEM, CATALOGUE_NAME, CATALOGUE_OWNER_NAME, CATALOGUEITEM_MAXIMUM_QUANTITY)
        SELECT CATALOGUEITEM_ID AS CATALOGUEITEM_ID, CATALOGUEITEM_CODE AS CATALOGUEITEM_CODE, CATALOGUEITEM_STATUS AS CATALOGUEITEM_STATUS, CATALOGUEITEM_DESCRIPTION AS CATALOGUEITEM_DESCRIPTION, CATALOGUEITEM_TYPE_CODE AS CATALOGUEITEM_TYPE_CODE, CATALOGUEITEM_TYPE_DESCRIPTION AS CATALOGUEITEM_TYPE_DESCRIPTION, CATALOGUEITEM_UNIT AS CATALOGUEITEM_UNIT, CATALOGUEITEM_CATEGORY AS CATALOGUEITEM_CATEGORY, CATALOGUEITEM_PRICE_PER_ITEM AS CATALOGUEITEM_PRICE_PER_ITEM, CATALOGUEITEM_BILLED_PER_ITEM AS CATALOGUEITEM_BILLED_PER_ITEM, CATALOGUE_NAME AS CATALOGUE_NAME, CATALOGUE_OWNER_NAME AS CATALOGUE_OWNER_NAME, CATALOGUEITEM_MAXIMUM_QUANTITY AS CATALOGUEITEM_MAXIMUM_QUANTITY 
        FROM ALAYACARE_${var.SF_ENVIRONMENT}UCTION."5C3EE5FE-45A5-4DA7-A35B-BD7CDAFE3548".SHARED_CATALOGUE_ITEMS;

	SELECT CONCAT(''Message : '',"number of rows inserted",'' Rows Inserted.'') into :return_result FROM TABLE(RESULT_SCAN(LAST_QUERY_ID()));
	return return_result;
END;

 EOT
}

